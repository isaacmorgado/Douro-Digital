<script>
(function() {
  var banner = document.querySelector('[fs-consent-element="banner"]');
  var prefs = document.querySelector('[fs-consent-element="preferences"]');
  if (localStorage.getItem('cookie_consent')) {
    if (banner) banner.style.display = 'none';
  }
  document.querySelectorAll('[fs-consent-element="allow"]').forEach(function(btn) {
    btn.addEventListener('click', function(e) {
      e.preventDefault();
      localStorage.setItem('cookie_consent', 'accepted');
      if (banner) banner.style.display = 'none';
      if (prefs) prefs.style.display = 'none';
    });
  });
  document.querySelectorAll('[fs-consent-element="deny"]').forEach(function(btn) {
    btn.addEventListener('click', function(e) {
      e.preventDefault();
      localStorage.setItem('cookie_consent', 'rejected');
      if (banner) banner.style.display = 'none';
      if (prefs) prefs.style.display = 'none';
    });
  });
  document.querySelectorAll('[fs-consent-element="open-preferences"]').forEach(function(btn) {
    btn.addEventListener('click', function(e) {
      e.preventDefault();
      if (prefs) prefs.style.display = prefs.style.display === 'none' ? '' : 'none';
    });
  });
  var submitBtn = document.querySelector('.fs-consent_prefs_submit');
  if (submitBtn) {
    submitBtn.addEventListener('click', function(e) {
      e.preventDefault();
      localStorage.setItem('cookie_consent', 'accepted');
      if (banner) banner.style.display = 'none';
      if (prefs) prefs.style.display = 'none';
    });
  }
})();
</script>
