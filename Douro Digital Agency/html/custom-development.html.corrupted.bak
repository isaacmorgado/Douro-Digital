}

/* Define the initial states */
.c-button_line, .c-button_embed, .c-button_embed.cc-up, .c-button p {
  transform: translateX(0);
  transition-delay: 0.1s;
}

.c-button_line-left, .c-button_embed-left {
  transform: translateX(-100%);
}

.c-button_embed-left.cc-up {
  transform: translate(-100%, 100%);
}

/* Define the hover states for buttons */
.c-button:hover .c-button_line, .c-button:hover .c-button_embed, 
.c-button:hover .c-button_embed.cc-up, .c-button:hover p {
  transform: translateX(100%);
  transition-delay: 0s;
}

.c-button:hover .c-button_line-left, .c-button:hover .c-button_embed-left {
  transform: translateX(0);
  transition-delay: 0.1s;
}

.c-button:hover .c-button_embed-left.cc-up {
  transform: translate(0, 0);
}

.c-button:hover .c-button_embed.cc-up {
  transform: translate(100%, -100%);
}

/* Transition for the text */
